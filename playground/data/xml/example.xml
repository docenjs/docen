<?xml version="1.0" encoding="UTF-8"?>
<!-- Example XML file with various data types -->
<root>
  <name>Example XML</name>
  <version>1.0.0</version>
  <description>A sample XML file with various data types</description>

  <!-- Author information -->
  <author>
    <name>John Doe</name>
    <email>john@example.com</email>
    <age>30</age>
  </author>

  <!-- Tags -->
  <tags>
    <tag>example</tag>
    <tag>xml</tag>
    <tag>data</tag>
  </tags>

  <!-- Settings -->
  <settings>
    <enabled>true</enabled>
    <maxItems>100</maxItems>
    <timeout>5000</timeout>
  </settings>

  <!-- Items list -->
  <items>
    <item>
      <id>1</id>
      <name>Item 1</name>
      <price>9.99</price>
      <inStock>true</inStock>
    </item>
    <item>
      <id>2</id>
      <name>Item 2</name>
      <price>19.99</price>
      <inStock>false</inStock>
    </item>
  </items>

  <!-- Metadata -->
  <metadata>
    <created>2024-01-01T00:00:00Z</created>
    <modified>2024-01-02T00:00:00Z</modified>
  </metadata>
</root> 