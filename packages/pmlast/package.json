{
  "name": "@docen/pmlast",
  "version": "0.0.0",
  "description": "PresentationML AST type definitions for PowerPoint documents - extends ooxast with PML-specific types",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepack": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "ooxml",
    "office",
    "powerpoint",
    "pptx",
    "ast",
    "xast",
    "xml",
    "typescript",
    "syntax-tree",
    "presentationml",
    "unist",
    "pml"
  ],
  "author": {
    "name": "Demo Macro",
    "email": "abc@imst.xyz",
    "url": "https://imst.xyz/"
  },
  "license": "MIT",
  "dependencies": {
    "@docen/ooxast": "workspace:*"
  }
}
