    "docx": "9.4.1",
    "exceljs": "4.4.0",
    "mammoth": "1.9.0",
    "node-html-parser": "7.0.1",
    "pdf-lib": "1.17.1",
    "pdf-parse": "1.1.1",
    "pptxgenjs": "3.12.0",
    "unpdf": "0.12.1",
    "xlsx": "0.18.5"
