{
  "name": "@docen/office",
  "version": "0.0.0",
  "description": "Office module for Docen - Processing for Office and PDF documents",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepack": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "office",
    "word",
    "excel",
    "powerpoint",
    "pdf",
    "docx",
    "xlsx",
    "pptx",
    "conversion",
    "extraction",
    "typescript"
  ],
  "author": {
    "name": "Demo Macro",
    "email": "abc@imst.xyz",
    "url": "https://imst.xyz/"
  },
  "license": "MIT",
  "dependencies": {
    "@docen/core": "workspace:*",
    "c12": "3.0.4",
    "defu": "6.1.4",
    "docx": "9.5.0",
    "fflate": "0.8.2",
    "image-meta": "0.2.1",
    "ofetch": "1.4.1",
    "pathe": "2.0.3",
    "pdf-lib": "1.17.1",
    "std-env": "3.9.0",
    "undio": "0.2.0",
    "unified-engine": "11.2.2",
    "unpdf": "1.0.5",
    "vfile": "6.0.3",
    "xast-util-from-xml": "4.0.0",
    "xast-util-to-string": "3.0.0",
    "xast-util-to-xml": "4.0.0",
    "xastscript": "4.0.0"
  },
  "devDependencies": {
    "@types/xast": "2.0.4"
  }
}
