{
  "name": "docen",
  "version": "0.0.0",
  "description": "Universal document conversion and processing library that works in any JavaScript runtime",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./cli": {
      "types": "./dist/cli/index.d.ts",
      "import": "./dist/cli/index.mjs",
      "require": "./dist/cli/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "bin": {
    "docen": "./dist/cli/index.mjs"
  },
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepack": "pnpm build"
  },
  "keywords": [
    "document",
    "conversion",
    "processing",
    "universal",
    "typescript",
    "cli",
    "framework",
    "markdown",
    "office",
    "pdf",
    "docx",
    "media",
    "data"
  ],
  "author": {
    "name": "Demo Macro",
    "email": "abc@imst.xyz",
    "url": "https://imst.xyz/"
  },
  "license": "MIT",
  "dependencies": {
    "@docen/core": "workspace:*",
    "@docen/data": "workspace:*",
    "@docen/document": "workspace:*",
    "@docen/media": "workspace:*",
    "@docen/office": "workspace:*",
    "@docen/providers": "workspace:*",
    "magic-regexp": "0.10.0",
    "pathe": "2.0.3",
    "std-env": "3.9.0",
    "unenv": "1.10.0",
    "unplugin": "2.3.2"
  },
  "devDependencies": {
    "size-limit": "11.2.0",
    "vitest": "3.1.2"
  }
}
